{
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": "error"
    },
    "plugins": [
        "jest"
    ],
    "env": {
        "jest/globals": true
    }
}